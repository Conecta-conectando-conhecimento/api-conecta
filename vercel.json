{
    "version": 2,
    "builds": [
        {
            "src": "./public/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "public/server.js"
        },
        {
         "src": "/api/auth",
         "dest": "public/routes/auth.js"
        }
    ]
}